{
  "schemaVersion": 1,
  "id": "waila",
  "version": "1.9.22",
  "custom": {
    "waila:plugins": [
      {
        "id": "waila:core",
        "initializer": "mcp.mobius.waila.addons.core.PluginCore"
      },
      {
        "id": "waila:vanilla",
        "initializer": "mcp.mobius.waila.addons.minecraft.PluginMinecraft"
      }
    ]
  },

  "entrypoints": {
    "main": [
      "mcp.mobius.waila.Waila"
    ],
    "client": [
      "mcp.mobius.waila.WailaClient"
    ],
    "modmenu": [
      "mcp.mobius.waila.WailaConfigModMenu"
    ]
  },
  "mixins": [
    {
      "config": "hwyla.client.json",
      "environment": "client"
    },
    {
      "config": "hwyla.server.json",
      "environment": "server"
    }
  ],

  "name": "Hwyla",
  "description": "What am I looking at?",
  "icon": "assets/waila/icon.png",
  "contact": {
    "homepage": "https://minecraft.curseforge.com/projects/hwyla",
